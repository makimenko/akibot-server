{
  "name": "akibot-server",
  "version": "0.0.1-SNAPSHOT",
  "description": "AkiBot Server",
  "repository": "https://github.com/makimenko/akibot-server.git",
  "license": "MIT",
  "scripts": {
    "start": "ts-node src/index.ts",
    "serve": "nodemon --watch src/**/*.ts --exec ts-node src/index.ts",
    "sandbox": "nodemon --watch src/**/*.ts --exec ts-node src/sandbox.ts",
    "sandbox2": "nodemon --watch src/**/*.ts --exec ts-node src/sandbox2.ts",
    "deploy": "tsc && xcopy /s /i /y dist r:\\akibot-server\\dist"
  },
  "dependencies": {
    "express": "^4.15.4",
    "http": "0.0.0",
    "typescript-logging": "^0.4.0",
    "ws": "^3.1.0"
  },
  "devDependencies": {
    "@types/express": "4.0.36",
    "@types/ws": "3.0.2",
    "typescript": "2.4.2"
  }
}
